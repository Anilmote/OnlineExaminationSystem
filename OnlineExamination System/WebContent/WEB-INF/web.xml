<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
id="WebApp_ID" version="3.0">

<servlet>
<servlet-name>staffQuestions</servlet-name>
<servlet-class>com.Servlet.staffQuestions</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>staffQuestions</servlet-name>
<url-pattern>/staff</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>AdminRegisterServlet</servlet-name>
<servlet-class>com.mankraft.AdminRegisterServlet</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>AdminRegisterServlet</servlet-name>
<url-pattern>/Admin</url-pattern>
</servlet-mapping>

</web-app>
